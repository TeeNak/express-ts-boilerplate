  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm"
      },
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "cwd": "${workspaceRoot}",
      "outFiles": [],
      "port": 9229,
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program (init)",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm"
      },
      "runtimeArgs": [
        "run-script",
        "debug-init"
      ],
      "cwd": "${workspaceRoot}",
      "outFiles": [],
      "port": 9229,
      "sourceMaps": true
    }
  ]
}
